package tdoubleking.test.item;

import net.minecraft.item.Item;
import net.minecraft.item.ItemStack;
import net.minecraft.item.ItemSword;
import tdoubleking.test.TestMod;

public class itemCheeseSword extends ItemSword{

	public itemCheeseSword(ToolMaterial material) {
		super(material);
		this.setDamage(new ItemStack(TestMod.cheeseSword), 1000);
		this.setMaxDamage(1000);
	}

}
